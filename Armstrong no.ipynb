   "source": [
    "# To check is given no is armstrong or not\n",
    "n=int(input(\"enter the number\"))\n",
    "num=n\n",
    "sum=0\n",
    "rem=0\n",
    "while(n!=0):\n",
    "    rem=n%10\n",
    "    sum=sum+rem*rem*rem\n",
    "    n=int(n/10)  \n",
    "if(num==sum):\n",
    "    print(\"armstrong no\")\n",
    "else:\n",
    "    print(\"not armstrong no\")"
   ]
