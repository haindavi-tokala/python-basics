   "source": [
    "class Point:\n",
    "\n",
    "    def __init__(self, x, y):\n",
    "\n",
    "        self.x = x\n",
    "\n",
    "        self.y = y\n",
    "\n",
    "    def __repr__(self):\n",
    "        self.c=self.x+self.y\n",
    "\n",
    "        return f\"Point(x={self.x}, y={self.y},{self.c})\"\n",
    "p=Point(10,29)   \n",
    "p.__repr__()"
   ]
